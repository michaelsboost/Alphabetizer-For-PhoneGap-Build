<?xml version='1.0' encoding='utf-8'?>
<widget xmlns = "http://www.w3.org/ns/widgets"
  xmlns:gap   = "http://phonegap.com/ns/1.0"
  id          = "com.michael.alphabetizer"
  version     = "1.0.2"
  versionCode = "6">
  <name>
    Alphabetizer
  </name>
  <description>
    Simply paste a list, and click alphabetize to alphabetize your list.
  </description>
  <author href="http://mikethedj4.github.io/" email="mikethedj4@yahoo.com">
    Michael Schwartz
  </author>

  <content src="index.html" />
  <access origin="*" />
  <preference name="permissions" value="none"/>
  <preference name="phonegap-version" value="cli-5.2.0" />
  <gap:plugin name="cordova-admob" source="npm" />

  <icon src="icon.png" gap:role="default" />
  <icon gap:platform="android" gap:qualifier="ldpi" src="res/icon/android/icon-36-ldpi.png" />
  <icon gap:platform="android" gap:qualifier="mdpi" src="res/icon/android/icon-48-mdpi.png" />
  <icon gap:platform="android" gap:qualifier="hdpi" src="res/icon/android/icon-72-hdpi.png" />
  <icon gap:platform="android" gap:qualifier="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
  <icon gap:platform="android" src="res/icon/android/icon-96-xhdpi.png" />
</widget>
